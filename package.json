{
  "name": "@comms-id/legal-documents",
  "version": "0.6.0",
  "description": "Centralised legal documents for Comms.ID platform",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/hooks",
    "USAGE.md",
    "RUNTIME_USAGE.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "node scripts/build.js",
    "prepublishOnly": "pnpm build",
    "test": "echo \"No tests configured\"",
    "clean": "rm -rf dist",
    "check-versions": "node scripts/check-versions.js",
    "install-hooks": "cp scripts/pre-commit .git/hooks/pre-commit && chmod +x .git/hooks/pre-commit"
  },
  "keywords": [
    "comms-id",
    "legal",
    "privacy",
    "terms",
    "policies",
    "compliance"
  ],
  "author": "Comms.ID",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/comms-id/policies.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "gray-matter": "^4.0.3",
    "marked": "^12.0.0",
    "puppeteer": "^24.16.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
